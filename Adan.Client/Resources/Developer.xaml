                                <EasingPointKeyFrame KeyTime="0:0:0.1" Value="-0.338,1.031"/>
                                <EasingPointKeyFrame KeyTime="0:0:0.2" Value="-0.127,1.05"/>
                                <EasingPointKeyFrame KeyTime="0:0:0.3" Value="0.003,1.06"/>
