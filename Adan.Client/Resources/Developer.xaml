                                <EasingPointKeyFrame KeyTime="0:0:0.5" Value="0.336,1.051"/>
                                <EasingPointKeyFrame KeyTime="0:0:0.6" Value="0.519,1.031"/>
                                <EasingPointKeyFrame KeyTime="0:0:0.7" Value="0.701,1.041"/>
                                <EasingPointKeyFrame KeyTime="0:0:0.8" Value="0.832,1.022"/>
                                <EasingPointKeyFrame KeyTime="0:0:0.9" Value="0.929,1.042"/>
                                <EasingPointKeyFrame KeyTime="0:0:1" Value="1.04,1.032"/>
                                <EasingPointKeyFrame KeyTime="0:0:1.1" Value="1.191,1.032"/>
                                <EasingPointKeyFrame KeyTime="0:0:1.2" Value="1.456,0.993"/>
