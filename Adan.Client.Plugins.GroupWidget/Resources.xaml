    <Style x:Key="SmallIcon"
           TargetType="Image">
        <Setter Property="Width"
                Value="20" />
        <Setter Property="Height"
                Value="20" />
        <Style.Triggers>
            <EventTrigger RoutedEvent="Binding.TargetUpdated">
                <BeginStoryboard>
                    <Storyboard>
                        <DoubleAnimation From="0"
                                         To="1"
                                         Duration="00:00:00.3"
                                         Storyboard.TargetProperty="Opacity"
                                         FillBehavior="Stop" />
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="MediumIcon"
           TargetType="Image">
        <Setter Property="Width"
                Value="40" />
        <Setter Property="Height"
                Value="40" />
    </Style>
    <DataTemplate x:Key="AffectDescriptionTemplate">
        <StackPanel Orientation="Horizontal"
                    HorizontalAlignment="Stretch"
                    Background="Transparent">
            <Image SnapsToDevicePixels="True"
                   Source="{Binding DefaultIcon, Converter={StaticResource AffectToIconConverter}, NotifyOnTargetUpdated=True}"
                   HorizontalAlignment="Right"
                   Name="AffectImage"
                   Style="{StaticResource SmallIcon}" />
            <Label Content="{Binding DisplayName}"
                   Margin="5,0,0,0"
                   FontSize="15"
                   VerticalAlignment="Center" />
        </StackPanel>
    </DataTemplate>
